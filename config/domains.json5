      },
      "price": {
        // Price container, Price with tax message, Discounted price, Regular price
        "element": ["form", "span", "span", "span"],
        "attribute": ["id", "class", "class", null],
        "name": ["ProductDetails","ProductPrice", "ProductPrice-final", null],
