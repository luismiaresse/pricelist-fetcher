// Identifiers needed to fetch elements in pages

{

  "elcorteingles": {
    "es": {
      "prod_name" : {
        "element": ["h1"],
        "attribute": ["id"],
        "name": ["js-product-detail-title"]
      },
      "brand": {
        "element": ["a"],
        "attribute": ["class"],
        "name": ["product_detail-brand"]
      },
      "price": {
        // Price container, Regular price, Discounted price
        "element": ["div", "p", "p"],
        "attribute": ["class", "class", "class"],
        "name": ["product_detail-buy-price-container", "price _big", "price _big _sale"],
        "iscontainer": [true, false, false]
      },
//      "size": {
//        "element": ["select", "option"],
//        "attribute": ["class"],
//        "name": ["js-select"]
//      },
      "color": {
        "element": ["span"],
        "attribute": ["id"],
        "name": ["colorNameSpan"]
      }
    }
  },


  "amazon": {
    "es,com": {
      "prod_name" : {
        "element": ["span"],
        "attribute": ["id"],
        "name": ["productTitle"]
      },
      "brand": {
        "element": ["a"],
        "attribute": ["id"],
        "name": ["bylineInfo"]
      },
      "price": {
        // Price container, Price (books), Price (any)
        "element": ["span", "span", "span"],
        "attribute": ["class", "id","class"],
        "name": ["(.*apexPriceToPay.*|.*priceToPay.*|.*reinventPriceAccordionT2.*)", "price", "a-offscreen"],
        "iscontainer": [true, false, false]
      }
    }
  },

  "pccomponentes": {
    "com": {
      "prod_name" : {
        "element": ["h1"],
        "attribute": ["class"],
        "name": ["h4"]
      },
      "brand": {
        // Product data container, Brand row
        "element": ["div", "div"],
        "attribute": ["class", "class"],
        "name": ["ficha-producto__datos-de-compra white-card-movil", "col-xs-12 col-sm-9"],
        "iscontainer": [true, false]
      },
      "price": {
        "element": ["div"],
        "attribute": ["id"],
        "name": ["precio-main"],
        "iscontainer": [false],
        "getfirst": [true]
      }
    }
  },

  "coolmod": {
    "com": {
      "prod_name" : {
        "element": ["h1"],
        "attribute": ["class"],
        "name": ["productTitle"]
      },
      "brand": {
        "element": ["div"],
        "attribute": ["class"],
        "name": ["productextrainfotext pb-xl-3 pb-md-2 pb-3"]
