  "worten": {
    "es,pt": {
      "prod_name" : {
        "element": ["h1"],
        "attribute": ["class"],
        "name": ["w-product__name iss-product-name"]
      },
      "brand": {
        // Info container, Brand
        "element": ["li", "span"],
